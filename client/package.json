{
  "name": "lidy-blog",
  "version": "1.0.0",
  "private": true,
  "description": "lidy-blog for express react",
  "scripts": {
    "start": "npm-run-all --sequential build start-prod",
    "dev": "npm-run-all --sequential build-server:dev start-dev",
    "build:dll": "webpack --config webpack.dll.config.js",
    "build:server": "webpack --colors --config webpack.config.dev.server.js",
    "build:client": "webpack --colors --config webpack.config.dev.client.js",
    "start-dev":"node src/server.bundle.js"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "mobx": "^2.4.3",
    "mobx-react": "^3.5.5",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.3.0",
    "react-addons-update": "^15.4.1",
    "react-bootstrap": "^0.30.6",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^1.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-core": "^6.13.2",
    "babel-jest": "^14.1.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.13.0",
    "babel-standalone": "^6.10.3",
    "body-parser": "1.9.0",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "ejs": "1.0.0",
    "file-loader": "^0.9.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "morgan": "1.3.1",
    "postcss-loader": "^0.9.1",
    "style-loader": "^0.13.0",
    "supervisor": "latest",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2",
    "whatwg-fetch": "^2.0.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  }
}
